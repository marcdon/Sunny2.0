<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="e.marc.sunny">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:icon,android:theme">
        <activity android:name=".UserloginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".sunnyPasswordActivity" />
        <activity android:name=".planner" />
        <activity android:name=".homepageActivity" />
        <activity android:name=".itemArrayAdapter" />
        <activity android:name=".boardActivity" />
        <activity android:name=".navigationActivity"/>
        <activity android:name=".BoardLocater" />
        <activity android:name=".SignupActivity" />
    </application>

</manifest>